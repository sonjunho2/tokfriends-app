{
  "expo": {
    "name": "딱친",
    "slug": "ddakchin",
    "owner": "sonfree",
    "scheme": "ddakchin",
    "version": "1.0.0",
    "orientation": "portrait",
    "assetBundlePatterns": ["**/*"],
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.sonfree.ddakchin",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#111827"
      }
      /* ⛔ 여기에 compileSdkVersion / targetSdkVersion / buildToolsVersion 넣지 마세요 */
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sonfree.ddakchin",
      "buildNumber": "1"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "minSdkVersion": 24
            /* "buildToolsVersion": "34.0.0"  // 필요 시 추가, 없어도 됩니다 */
          }
        }
      ]
    ],
    "extra": {
      "apiBaseUrl": "https://tok-friends-api.onrender.com",
      "eas": { "projectId": "101718db-f90c-4fc8-9b2f-ed06405d9991" }
    }
  }
}
